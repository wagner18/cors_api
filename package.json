  "scripts": {
    "touch-files": "echo '' >> src/api/config/lib-assets.js && echo '' >> src/api/config/bundle-assets.js",
    "clean": "rm -rf lib && rm -rf dist",
    "build:dll": "webpack --config=webpack.dll.config.js",
    "babel": "BABEL_ENV=server babel src/ -d dist",
    "build": "npm run clean && npm run build:dll && webpack --progress --colors && npm run babel",
    "babel-node": "NODE_ENV=development BABEL_ENV=server babel-node src/app.js",
    "babel-watch": "NODE_ENV=development BABEL_ENV=server babel-watch -D -w src/app.js -w src/api -w src/ssr/index.js -x src/api/config/bundle-assets.js -x src/api/config/lib-assets.js src/app.js",
    "dev": "npm run clean && npm run touch-files && NODE_ENV=development npm run build:dll && npm run babel-watch",
    "start": "node dist/app.js",
    "lint": "eslint .",
    "test": "lab -T loaders/babel-loader.js --linter node_modules/.bin/eslint --verbose --sourcemaps",
    "test-cov-html": "lab -T loaders/babel-loader.js -r html -o coverage.html"
  },
